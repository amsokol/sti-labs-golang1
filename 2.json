{
    "kind": "List",
    "apiVersion": "v1",
    "metadata": {},
    "items": [
        {
            "kind": "DeploymentConfig",
            "apiVersion": "v1",
            "metadata": {
                "name": "sti-labs-golang1",
                "labels": {
                    "app": "sti-labs-golang1"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange",
                        "imageChangeParams": {
                            "automatic": true,
                            "containerNames": [
                                "sti-labs-golang1"
                            ],
                            "from": {
                                "kind": "ImageStreamTag",
                                "name": "sti-labs-golang1:latest"
                            }
                        }
                    }
                ],
                "replicas": 1,
                "selector": {
                    "deploymentconfig": "sti-labs-golang1"
                },
                "template": {
                    "metadata": {
                        "labels": {
                            "deploymentconfig": "sti-labs-golang1"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "name": "sti-labs-golang1",
                                "image": "labs/sti-labs-golang1:latest",
                                "ports": [
                                    {
                                        "containerPort": 8080,
                                        "protocol": "TCP"
                                    }
                                ]
                            }
                        ]
                    }
                }
            }
        },
        {
            "kind": "Service",
            "apiVersion": "v1",
            "metadata": {
                "name": "sti-labs-golang1",
                "labels": {
                    "app": "sti-labs-golang1"
                }
            },
            "spec": {
                "ports": [
                    {
                        "name": "8080-tcp",
                        "protocol": "TCP",
                        "port": 8080,
                        "targetPort": 8080,
                        "nodePort": 0
                    }
                ],
                "selector": {
                    "deploymentconfig": "sti-labs-golang1"
                }
            }
        },
	{
	    "kind": "Route",
	    "apiVersion": "v1",
	    "metadata": {
		"name": "sti-labs-golang1",
    		"namespace": "labs",
    		"labels": {
        	    "app": "sti-labs-golang1"
    		}
	    },
	    "spec": {
    		"host": "sti-labs-golang1.openshift.beeline",
    		"to": {
        	    "kind": "Service",
        	    "name": "sti-labs-golang1"
    		}
	    }
	}
    ]
}

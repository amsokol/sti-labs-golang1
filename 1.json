{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
      {
          "kind": "ImageStream",
          "apiVersion": "v1",
          "metadata": {
              "name": "base-centos7",
              "namespace": "labs",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          },
          "spec": {
              "dockerImageRepository": "openshift/base-centos7"
          }
      },
      {
          "kind": "ImageStream",
          "apiVersion": "v1",
          "metadata": {
              "name": "sti-labs-golang1-builder",
              "namespace": "labs",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          }
      },
      {
          "kind": "ImageStream",
          "apiVersion": "v1",
          "metadata": {
              "name": "sti-labs-golang1",
              "namespace": "labs",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          }
      },
      {
          "kind": "BuildConfig",
          "apiVersion": "v1",
          "metadata": {
              "name": "sti-labs-golang1-builder",
              "namespace": "labs",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          },
          "spec": {
              "source": {
                  "type": "Git",
                  "git": {
                      "uri": "https://github.com/amsokol/sti-labs-golang1.git"
                  },
                  "contextDir": "S2I"
              },
              "strategy": {
                  "type": "Docker",
                  "dockerStrategy": {
                      "from": {
                          "kind": "ImageStreamTag",
                          "name": "base-centos7:latest"
                      }
                  }
              },
              "output": {
                  "to": {
                      "kind": "ImageStreamTag",
                      "name": "sti-labs-golang1-builder:latest"
                  }
              }
          }
      },
      {
          "kind": "BuildConfig",
          "apiVersion": "v1",
          "metadata": {
              "name": "sti-labs-golang1",
              "namespace": "labs",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          },
          "spec": {
              "source": {
                  "type": "Git",
                  "git": {
                      "uri": "https://github.com/amsokol/sti-labs-golang1.git"
                  }
              },
              "strategy": {
                  "type": "Source",
                  "sourceStrategy": {
                      "from": {
                          "kind": "ImageStreamTag",
                          "namespace": "labs",
                          "name": "sti-labs-golang1-builder:latest"
                      }
                  }
              },
              "output": {
                  "to": {
                      "kind": "ImageStreamTag",
                      "name": "sti-labs-golang1:latest"
                  }
              }
          }
      }
    ]
}

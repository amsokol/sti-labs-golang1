{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
      {
          "kind": "ImageStream",
          "apiVersion": "v1",
          "metadata": {
              "name": "base-centos7",
              "namespace": "labs",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          },
          "spec": {
              "dockerImageRepository": "openshift/base-centos7"
          }
      },
      {
          "kind": "ImageStream",
          "apiVersion": "v1",
          "metadata": {
              "name": "sti-labs-golang1-builder",
              "namespace": "labs",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          }
      },
      {
          "kind": "ImageStream",
          "apiVersion": "v1",
          "metadata": {
              "name": "sti-labs-golang1",
              "namespace": "labs",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          }
      },
      {
          "kind": "BuildConfig",
          "apiVersion": "v1",
          "metadata": {
              "name": "sti-labs-golang1-builder",
              "namespace": "labs",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          },
          "spec": {
              "source": {
                  "type": "Git",
                  "git": {
                      "uri": "https://github.com/amsokol/sti-labs-golang1.git"
                  },
                  "contextDir": "S2I"
              },
              "strategy": {
                  "type": "Docker",
                  "dockerStrategy": {
                      "from": {
                          "kind": "ImageStreamTag",
                          "name": "base-centos7:latest"
                      }
                  }
              },
              "output": {
                  "to": {
                      "kind": "ImageStreamTag",
                      "name": "sti-labs-golang1-builder:latest"
                  }
              }
          }
      },
      {
          "kind": "BuildConfig",
          "apiVersion": "v1",
          "metadata": {
              "name": "sti-labs-golang1",
              "namespace": "labs",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          },
          "spec": {
              "source": {
                  "type": "Git",
                  "git": {
                      "uri": "https://github.com/amsokol/sti-labs-golang1.git"
                  }
              },
              "strategy": {
                  "type": "Source",
                  "sourceStrategy": {
                      "from": {
                          "kind": "ImageStreamTag",
                          "namespace": "labs",
                          "name": "sti-labs-golang1-builder:latest"
                      }
                  }
              },
              "output": {
                  "to": {
                      "kind": "ImageStreamTag",
                      "name": "sti-labs-golang1:latest"
                  }
              }
          }
      },
      {
          "kind": "DeploymentConfig",
          "apiVersion": "v1",
          "metadata": {
              "name": "sti-labs-golang1",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          },
          "spec": {
              "triggers": [
                  {
                      "type": "ConfigChange"
                  },
                  {
                      "type": "ImageChange",
                      "imageChangeParams": {
                          "automatic": true,
                          "containerNames": [
                              "sti-labs-golang1"
                          ],
                          "from": {
                              "kind": "ImageStreamTag",
                              "name": "sti-labs-golang1:latest"
                          }
                      }
                  }
              ],
              "replicas": 1,
              "selector": {
                  "deploymentconfig": "sti-labs-golang1"
              },
              "template": {
                  "metadata": {
                      "labels": {
                          "deploymentconfig": "sti-labs-golang1"
                      }
                  },
                  "spec": {
                      "containers": [
                          {
                              "name": "sti-labs-golang1",
                              "image": "labs/sti-labs-golang1:latest",
                              "ports": [
                                  {
                                      "containerPort": 8080,
                                      "protocol": "TCP"
                                  }
                              ]
                          }
                      ]
                  }
              }
          }
      },
      {
          "kind": "Service",
          "apiVersion": "v1",
          "metadata": {
              "name": "sti-labs-golang1",
              "labels": {
                  "app": "sti-labs-golang1"
              }
          },
          "spec": {
              "ports": [
                  {
                      "name": "8080-tcp",
                      "protocol": "TCP",
                      "port": 8080,
                      "targetPort": 8080,
                      "nodePort": 0
                  }
              ],
              "selector": {
                  "deploymentconfig": "sti-labs-golang1"
              }
          }
      },
      {
          "kind": "Route",
          "apiVersion": "v1",
          "metadata": {
            "name": "sti-labs-golang1",
            "namespace": "labs",
            "labels": {
              "app": "sti-labs-golang1"
            }
          },
          "spec": {
            "host": "sti-labs-golang1.openshift.beeline",
            "to": {
              "kind": "Service",
              "name": "sti-labs-golang1"
            }
          }
        }
    ]
}
